# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  id: ID!
  username: String!
  firstName: String!
  lastName: String!
  email: String!
  phone: String
  region: String
}

type Query {
  me: User
  users: User
}

input UserInput {
  username: String!
  firstName: String!
  lastName: String!
  email: String!
  phone: String
  region: String
}

type Mutation {
  addUser(input: UserInput!): User!
  editUser(userId: String!, input: UserInput!): User!
}